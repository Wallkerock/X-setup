# cosmic_latte_light fish shell theme by nightsense (https://github.com/nightsense/cosmic_latte)

# normal text
set fish_color_host              485a62
set fish_color_normal            485a62
set fish_pager_color_completion  485a62
set fish_pager_color_description 485a62

# muted text
set fish_color_autosuggestion    63757e
set fish_color_comment           63757e

# reverse muted
set fish_pager_color_progress    fff8e7 --background=#63757e

# underlined text
set fish_color_valid_path        --underline

# highlit text
set fish_color_match             --background=#b1b4a2
set fish_color_search_match      --background=#b1b4a2
set fish_color_selection         --background=#b1b4a2

# red errors/warnings
set fish_color_cancel            c44756
set fish_color_cwd_root          c44756
set fish_color_error             c44756

# gold special elements
set fish_color_redirection       916d03
set fish_color_escape            916d03
set fish_color_operator          916d03
set fish_color_end               916d03

# green commands/status
set fish_color_command           1f8332
set fish_color_cwd               1f8332
set fish_color_user              1f8332

# cyan parameters
set fish_color_param             007f8a

# blue quoted strings
set fish_color_quote             0075c9

# magenta pager name match
set fish_pager_color_prefix      a154ae
