# cosmic_latte_dark fish shell theme by nightsense (https://github.com/nightsense/cosmic_latte)

# normal text
set fish_color_host              abb0c0
set fish_color_normal            abb0c0
set fish_pager_color_completion  abb0c0
set fish_pager_color_description abb0c0

# muted text
set fish_color_autosuggestion    898f9e
set fish_color_comment           898f9e

# reverse muted
set fish_pager_color_progress    202a31 --background=#898f9e

# underlined text
set fish_color_valid_path        --underline

# highlit text
set fish_color_match             --background=#4c5764
set fish_color_search_match      --background=#4c5764
set fish_color_selection         --background=#4c5764

# red errors/warnings
set fish_color_cancel            c17b8d
set fish_color_cwd_root          c17b8d
set fish_color_error             c17b8d

# gold special elements
set fish_color_redirection       b28761
set fish_color_escape            b28761
set fish_color_operator          b28761
set fish_color_end               b28761

# green commands/status
set fish_color_command           7d9761
set fish_color_cwd               7d9761
set fish_color_user              7d9761

# cyan parameters
set fish_color_param             459d90

# blue quoted strings
set fish_color_quote             5496bd

# magenta pager name match
set fish_pager_color_prefix      9b85bb
