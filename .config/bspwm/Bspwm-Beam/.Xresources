Xft.dpi:         96
Xft.antialias:   true
Xft.hinting:     true
Xft.rgba:        rgb
Xft.autohint:    false
Xft.hintstyle:   hintslight
Xft.lcdfilter:   lcddefault

! ------------------------------------------------------------------------------
! ROFI setup
! ------------------------------------------------------------------------------

rofi.separator-style:   solid
rofi.font:              Roboto Condensed 14
rofi.color-enabled:     true
rofi.padding:           50
rofi.line-margin:       5
rofi.lines:             10
rofi.width:             720
rofi.disable-history:   true
rofi.modi:              drun,window,run,keys
rofi.sidebar-mode:      true
rofi.fullscreen:        true

! ------------------------------------------------------------------------------
! ALL setup
! ------------------------------------------------------------------------------

!*fading:                         0
!*fadeColor:                      #EEE8D5
*pointerColorBackground:          #2B2C2B
*pointerColorForeground:          #16A085

! ------------------------------------------------------------------------------
! RXVT UNICODE setup
! ------------------------------------------------------------------------------

*.foreground:                 #171717
*.background:                 #CFD6D2
*.cursorColor:                #171717

!! black dark/light
*color0:                          #EAEAEA
*color8:                          #7A7A7A

!! red dark/light
*color1:                          #BA2249
*color9:                          #CC2C58

!! green dark/light
*color2:                          #323232
*color10:                         #363636

!! yellow dark/light
*color3:                          #323232
*color11:                         #C01C4F

!! blue dark/light
*color4:                          #CFD6D2
*color12:                         #00877C

!! magenta dark/light
*color5:                          #2E2E2E
*color13:                         #57A49E

!! cyan dark/light
*color6:                          #26988F
*color14:                         #30918B

!! white dark/light
*color7:                          #5A5A5A
*color15:                         #171717

!-------------------------------------------
! RXVT UNICODE appearance
!-------------------------------------------

Xcursor.theme: Breeze
Xcursor.size:                   0

*.geometry:					150x45
!*.internalBorder:			10
!*.font:					xft:Iosevka:size=10
*.font:						xft:Ubuntu Mono:size=12

! alternative font settings with 'terminus':
!URxvt.font: -*-fixed-*-r-normal--*-*-*-*-*-*-iso10646-*
!URxvt.font:      -xos4-terminus-medium-r-normal--16-160-72-72-c-80-iso10646-1
!URxvt.bold.font: -xos4-terminus-bold-r-normal--16-160-72-72-c-80-iso10646-1
!! terminus names see end of file!

URxvt.depth:                      32
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*cursorUnderline:            false
URxvt*saveLines:                  5000
!URxvt*lineSpace:                  0.1
URxvt*letterSpace:                -1
!!! perl extension
Rxvt.perl-ext-common:             default,matcher,resize-font,url-select,keyboard-select,clipboard
URxvt.resize-font.smaller:        C-Down
URxvt.resize-font.bigger:         C-Up
!!! copy/paste
URxvt.clipboard.autocopy:         true
URxvt.keysym.M-c:                 perl:clipboard:copy
URxvt.keysym.M-v:                 perl:clipboard:paste
URxvt.iconFile:    /usr/share/icons/Clarity/scalable/apps/terminal.svg

